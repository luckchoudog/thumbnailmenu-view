<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- ThumbnailMenu -->
    <declare-styleable name="ThumbnailMenu">
        <attr name="thumbnailmenu_position" format="enum">
            <enum name="left" value="1000" />
            <enum name="bottom" value="1001" />
            <enum name="right" value="1002" />
        </attr>
        
        <attr name="thumbnailmenu_zoom" format="float" />
        <attr name="thumbnailmenu_iscolseinoutside" format="boolean" />
    </declare-styleable>

</resources>
